if game.PlaceId == 11542692507 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("EZ NOOB HUB", "DarkTheme")
    local Tab = Window:NewTab("Main Tab")
    local Section = Tab:NewSection("Auto Farm")
    
    getgenv().Toggled = false

    Section:NewToggle("Auto Farm", "This Will Farm Aokiji!", function(state)
        if state then
            getgenv().autofarm = true
            while getgenv().autofarm == true do wait()
                    tweenService = game:GetService("TweenService")
                    tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Quart)
                    A = game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart
                    tween = tweenService:Create(A, tweenInfo, {CFrame = CFrame.new(-406.0546569824219, 5, 940.1435546875)})
                    tween:Play()
            end
        else
            getgenv().autofarm = false
        end
    end)

    
    Section:NewToggle("Auto Click", "This Will Click Automaticly For You! (Must Enable With Auto Click)", function(state)
        if state then
            getgenv().autoclick = true
            while getgenv().autoclick == true do wait()
                game:GetService'VirtualUser':Button1Down(Vector2. new(1,1))
            end
        else
            getgenv().autoclick = false
        end
    end)

    Section:NewToggle("Auto Buy Egg", "This Will Buy Egg At Spawn :)", function(state)
        if state then
            getgenv().autobuyegg = true
            while getgenv().autobuyegg == true do wait()
                tweenService = game:GetService("TweenService")
                tweenInfo = TweenInfo.new(3, Enum.EasingStyle.Quart)
                A = game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart
                tween = tweenService:Create(A, tweenInfo, {CFrame = CFrame.new(-455.04058837890625, 0, 757.2879028320312)})
                tween:Play()
                wait(3)
                game:GetService("ReplicatedStorage").Remotes.Server:FireServer({"BuyHeroes","Pyecy Village"})
            end
        else
            getgenv().autobuyegg = false
        end
    end)

elseif game.PlaceId == 3956818381 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("EZ NOOB HUB", "DarkTheme")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Auto Farm")

Section:NewToggle("Auto Click", "This will click automaticly fo you!", function(state)
    if state then
        getgenv().swing = true
        while getgenv().swing == true do wait()
        game:GetService("Players").LocalPlayer.ninjaEvent:FireServer("swingKatana")
        end
        else
        getgenv().swing = false
    end
end)

Section:NewToggle("Auto Sell", "This will teleport you to sell", function(state)
    if state then
        getgenv().tp1 = true
        while getgenv().tp1 == true do wait(1)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(73.85891723632812, 2.999999523162842, -46.56789779663086)
        wait(4)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(73.85891723632812, 15, -46.56789779663086)
        end
    else
        getgenv().tp1 = false
    end
end)


Section:NewToggle("Auto Buy Sword And Belts", "This will buy swords and belts automaticly!", function(state)
    if state then
        getgenv().autobuy = true 
        while getgenv().autobuy == true do wait()
            game:GetService("Players").LocalPlayer.ninjaEvent:FireServer("buyAllSwords","Blazing Vortex Island")
            game:GetService("Players").LocalPlayer.ninjaEvent:FireServer("buyAllBelts","Blazing Vortex Island")
         end
    else
        getgenv().autobuy = false
    end
end)

local Section = Tab:NewSection("Island TP")

