if game.PlaceId == 10232731562 then

    local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
    local venyx = library.new("EZ NOOB HUB", 5013109572)
    
    -- Themes
    
    local themes = {
    Background = Color3.fromRGB(33, 41, 61),
    Glow = Color3.fromRGB(255, 255, 255),
    Accent = Color3.fromRGB(33, 41, 61),
    LightContrast = Color3.fromRGB(30, 43, 74),
    DarkContrast = Color3.fromRGB(33, 41, 61),  
    TextColor = Color3.fromRGB(255, 255, 255)
    }
    
    -- Anti AFK
    
    spawn(function()
        local function antiafk()
            plr.Idled:connect(function()
                game:service("VirtualUser"):CaptureController()
                game:service("VirtualUser"):ClickButton2(Vector2.new())
            end)
            warn("Anti AFK Enabled")
         end
    end)
    
    
    -- function check level
    
    getgenv().mylvl = game:GetService("Players").LocalPlayer.Data.Level.Value
    
    
    -- function noclip
    
     function noclips()
        spawn(function()
            if getgenv().autofarm == true then
                game:GetService("RunService").Stepped:Connect(function()
                    if getgenv().autofarm == true then
                        pcall(function()
                            for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                                    if v:IsA("BasePart") then
                                    v.CanCollide = false
                                end
                            end
                        end)
                    elseif getgenv().autofarm == false then              
                            for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                                if v:IsA("BasePart") then
                                    v.CanCollide = true
                            end
                        end
                    end
                end)
            end
        end)
    end
    
    -- Function Auto Quest
    
    function autoquest()
        spawn(function()
            while getgenv().autofarm == true do wait()
                pcall(function()
                    if game:GetService("Players").LocalPlayer.PlayerGui.MainGui.QuestBar.Visible == false then
                        if mob == "Thief" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 1})
                        elseif mob == "Buff Thief" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 3})
                        elseif mob == "Desert Thief" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 2})
                        elseif mob == "Desert Swordsman" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 4})
                        elseif mob == "Monkey" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 5})
                        elseif mob == "Buff Monkey" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 6})
                        elseif mob == "Blizzard bandit" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 7})
                        elseif mob == "Yeti" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 8})
                        elseif mob == "Mink" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 9})
                        elseif mob == "Pro Mink" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 10})
                        elseif mob == "Pirate" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 11})
                        elseif mob == "Pro Pirate" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 12})
                        elseif mob == "Sharkman Inhabitant" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 13})
                        elseif mob == "Pro Sharkman" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 14})
                        elseif mob == "Arshort Boss" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 15})
                        elseif mob == "Nob Marine" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 16})
                        elseif mob == "Marine Swordsman" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 17})
                        elseif mob == "Pro Marine" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 18})
                        elseif mob == "Morman" then
                        game:GetService("Players").LocalPlayer.PlayerGui.DialougeGui.DialogModule.Sending:FireServer({["Key"] = "GetQuest",["QuestID"] = 21})
                        end
                    end
                end)
            end
        end)
    end
    
    -- Function Auto Farm
    
    function autofarm()
        spawn(function()
            while getgenv().autofarm == true do wait()  
                pcall(function()
                    for i,v in pairs(game:GetService("Workspace").World.Live:GetDescendants()) do
                        if v.Name == mob then
                            if v.Humanoid.Health >= 0 then
                                repeat wait()
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,5,slider or 9)
                                until getgenv().autofarm == false or v.Humanoid.Health <= 0
                            end
                        end
                    end
                end)
            end
        end)
    end 
    
    -- function autoclick
    
    function click()
        spawn(function()        
            while getgenv().autofarm == true do wait(0.5)
            pcall(function()
                local virtual_input_manager = game:GetService("VirtualInputManager")
                local x, y = 0, 0 
                virtual_input_manager:SendMouseButtonEvent(x, y, 0, false, game, 0)
                virtual_input_manager:SendMouseButtonEvent(x, y, 0, true, game, 0)
                end)
            end
        end)
    end
    
    -- Auto Equip
    
    equiplist = {}
    equip = nil
    
    spawn(function()
        for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
            if v.Name ~= "GuiHandler" then
                table.insert(equiplist, v.Name)
            end
        end
    end)
    
    spawn(function()
        while wait() do
            if autoequip then
                    pcall(function()
                        game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(equip))
                end)
            end
        end
    end)
    
    -- Main
    
    local Main = venyx:addPage("Main", 5012544693)
    local section1 = Main:addSection("Auto Farm")
    local section2 = Main:addSection("Auto Equip")
    local section3 = Main:addSection("Auto Stats")
    
    section1:addToggle("Auto Farm", nil, function(value)
            if value then
            getgenv().autofarm = true
            autofarm()
            autoquest()
            click()
            noclips()
            else
            getgenv().autofarm = false
        end
    end)
    
    section1:addDropdown("Select Mob", {"Thief Lvl. 1", "Desert Thief Lvl. 20", "Desert Swordsman Lvl. 30", "Monkey Lvl. 50",
    "Blizzard Bandit Lvl. 75", "Yeti Lvl. 90", "Mink Lvl. 110", "Pro Mink Lvl. 150", "Pirate Lvl. 200", "Pro Pirate Lvl. 250",
    "Sharkman Inhabitant Lvl. 300", "Pro Sharkman Lvl. 325", "Marine Swordsman Lvl. 350", "Nob Marine Lvl. 375", "Pro Marine Lvl. 400"}, function(text)
        if text == "Thief Lvl. 1" then
            mob = "Thief"
        elseif text == "Desert Thief Lvl. 20" then
            mob = "Desert Thief"
        elseif text == "Desert Swordsman Lvl. 30" then
            mob = "Desert Swordsman"
        elseif text == "Monkey Lvl. 50" then
            mob = "Monkey"
        elseif text == "Blizzard Bandit Lvl. 75" then
            mob = "Blizzard bandit"
        elseif text == "Yeti Lvl. 90" then
            mob = "Yeti"
        elseif text == "Mink Lvl. 110" then
            mob = "Mink"
        elseif text == "Pro Mink Lvl. 150" then
            mob = "Pro Mink"
        elseif text == "Pirate Lvl. 200" then
            mob = "Pirate"
        elseif text == "Pro Pirate Lvl. 250" then
            mob = "Pro Pirate"
        elseif text == "Sharkman Inhabitant Lvl. 300" then
            mob = "Sharkman Inhabitant"
        elseif text == "Pro Sharkman Lvl. 325" then
            mob = "Pro Sharkman"
        elseif text == "Marine Swordsman Lvl. 350" then
            mob = "Marine Swordsman"
        elseif text == "Nob Marine Lvl. 375" then
            mob = "Nob Marine"
        elseif text == "Pro Marine Lvl. 400" then
            mob = "Pro Marine"
        end
    end)
    
    section1:addDropdown("Select Boss", {"Buff Thief Lvl. 10" ,"Buff Monkey Lvl. 60", "Arshort Boss Lvl. 350","Morman Lvl. 425"}, function(text)
        if text == "Buff Thief Lvl. 10" then
            mob = "Buff Thief"
        elseif text == "Buff Monkey Lvl. 60" then
            mob = "Buff Monkey"
        elseif text == "Arshort Boss Lvl. 350" then
            mob = "Arshort Boss"
        elseif text == "Morman Lvl. 425" then
            mob = "Morman"
        end
    end)


    section1:addSlider("Auto Farm Distance", 9, 0, 20, function(value)
            slider = value
    end)
    
    section2:addToggle("Auto Equip", nil, function(value)
        autoequip = value
    end)
    
    
    section2:addDropdown("Select Item", equiplist, function(text)
        equip = text
    end)
    
    section2:addButton("Refresh Item", function()
        table.clear(equiplist)
        for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
            if v.Name ~= "GuiHandler" then
                table.insert(equiplist, v.Name)
            end
        end
    end)
    
    
    section3:addDropdown("Select Stats", {"Strength", "Defense", "Stamina", "Devil Fruit", "Weapon", "Gun"}, function(text)
        getgenv().selectstats = text
        if getgenv().selectstats == "Strength" then
             stats = "Combat"
        elseif getgenv().selectstats == "Defense" then
             stats = "Defense"
        elseif getgenv().selectstats == "Stamina" then
             stats = "Stamina"
        elseif getgenv().selectstats == "Devil Fruit" then
             stats = "DevilFruit"
        elseif getgenv().selectstats == "Weapon" then
             stats = "Weapon"
        elseif getgenv().selectstats == "Gun" then
             stats = "Gun"
        end
    end)
    
    section3:addToggle("Auto Add Stats", nil, function(value)
        if value then
            getgenv().addstats = value
            while  getgenv().addstats == true do wait(1)
                pcall(function()
                    game:GetService("ReplicatedStorage").Remotes.StatsRemote:FireServer(stats,1)
                end)
            end
        else
            getgenv().addstats = false
        end
    end)
    
    -- Misc
    
    local Misc = venyx:addPage("Misc", 5012544693)
    local section4 = Misc:addSection("Fruits OP")
    local section5 = Misc:addSection("Server")
    local section6 = Misc:addSection("FPS Boost")
    
    section4:addButton("Teleport To Natural Spawn Fruits", function()
        for i,v in pairs(game:GetService("Workspace").DFSpawns:GetDescendants())do
            if v.Name == "Handle" then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                v.Anchored = false
            end
        end  
    end)
    
    section4:addButton("Bring Dropped Fruits", function()
        for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
            if v.ClassName == "Tool" then
                v.Handle.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
            end
        end
    end)
    
    section5:addButton("Server Hop", function()
        local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/10232731562/servers/Public?sortOrder=Asc&limit=100"))
        for i,v in pairs(Servers.data) do
            if v.playing ~= v.maxPlayers then
                game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
            end
        end
    end)
    
    
    section5:addButton("Rejoin Server", function()
        game:GetService('TeleportService'):Teleport(game.PlaceId, game.Players.LocalPlayer)
    end)
    
    section6:addButton("FPS Booster", function()
        for i,v in next, workspace:GetDescendants() do
            if v:IsA("BasePart") then
                v.Material = Enum.Material.SmoothPlastic
            end
        end
    end)
    
    section6:addToggle("Disable 3DRendering", nil, function(value)
        if value then
            getgenv().render = value
            local RunService = game:GetService("RunService")
            RunService:Set3dRenderingEnabled(false)
        else 
            getgenv().render = false
            local RunService = game:GetService("RunService")
            RunService:Set3dRenderingEnabled(true)
        end
    end)
    
    
    -- Gui
    
    local Theme = venyx:addPage("Gui", 5012544693)
    local section7 = Theme:addSection("FPS Boost")
    local colors = Theme:addSection("Colors")
    
    section7:addKeybind("Toggle Keybind", Enum.KeyCode.LeftControl, function()
    venyx:toggle()
        end, function()
    end)
    
    for theme, color in pairs(themes) do
            colors:addColorPicker(theme, color, function(color3)
            venyx:setTheme(theme, color3)
        end)
    end
    
    -- Load
    venyx:SelectPage(venyx.pages[1], true)

elseif game:GetService("Players").LocalPlayer.PlayerGui.MainUI.Inventory.Slot1.Visible == true then
    
    -- notification 
    
    game.StarterGui:SetCore("SendNotification", {
        Title = "EZ NOOB HUB"; -- the title (ofc)
        Text = "Script Executed"; -- what the text says (ofc)
        Icon = ""; -- the image if u want. 
        Duration = 5; -- how long the notification should in secounds
        })
    
    -- selectmap
    
    if getgenv().selectmap == "Namek" then
        map = 1
    elseif getgenv().selectmap == "Yaiba" then
        map = 2
    elseif getgenv().selectmap == "Bleach" then
        map = 3
    elseif getgenv().selectmap == "Naruto" then
        map = 4
    elseif getgenv().selectmap == "One Piece" then
        map = 5      
    end
    
    if getgenv().mapinf == "Regular" then
        map2 = "Infinite"
    else
        map2 = "The Sanctuary"
    end
    
    -- auto join 
    
    spawn(function()
        pcall(function()
            if getgenv().joinstory == true then
                if game.PlaceId == 11600882147 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(15326.5479, -50.1157227, 2248.34619, 0, 0, 1, 1, 0, 0, 0, 1, 0)
                    wait(2)
                    game:GetService("ReplicatedStorage").Remotes.ChangeText:FireServer(workspace.Teleporters.Any,map)       
                    wait(2)
                    firesignal(game:GetService("Players").LocalPlayer.PlayerGui.TeleportUI.StartButton.MouseButton1Click)
                    wait(getgenv().delay)
                    local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/11600882147/servers/Public?sortOrder=Asc&limit=100"))
                    for i,v in pairs(Servers.data) do
                        if v.playing ~= v.maxPlayers then
                            game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
                        end
                    end
                end
            end
        end)
    end)
    
    spawn(function()
        pcall(function()
            if getgenv().joininf == true then
                if game.PlaceId == 11600882147 then 
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(15326.5479, -50.1157227, 2248.34619, 0, 0, 1, 1, 0, 0, 0, 1, 0)
                    wait(2)
                    game:GetService("ReplicatedStorage").Remotes.ChangeText:FireServer(workspace.Teleporters.Any,map2)       
                    wait(2)
                    firesignal(game:GetService("Players").LocalPlayer.PlayerGui.TeleportUI.StartButton.MouseButton1Click)
                    wait(getgenv().delay)
                    local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/11600882147/servers/Public?sortOrder=Asc&limit=100"))
                    for i,v in pairs(Servers.data) do
                        if v.playing ~= v.maxPlayers then
                            game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
                        end
                    end
                end
            end
        end)
    end)
    
    -- redeem codes
    
    spawn(function()
        if getgenv().code == true then
            game:GetService("ReplicatedStorage").Remotes.ClaimCode:InvokeServer("10KLIKES")
            wait(0.5)
            game:GetService("ReplicatedStorage").Remotes.ClaimCode:InvokeServer("100KGROUPMEMBERS")
            wait(0.5)
            game:GetService("ReplicatedStorage").Remotes.ClaimCode:InvokeServer("IXCook")
            wait(0.5)
            game:GetService("ReplicatedStorage").Remotes.ClaimCode:InvokeServer("WCKD!")
        end
    end)
    
    -- get player unit 
    
    function getunit()
        pcall(function()
            random = math.random(1,6)
            slot = random
            for i,v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do wait()
                if v.DataCost == 34 then
                    v.Name = "Player"
                    if slot == 1 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot1.Value
                    elseif slot == 2 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot2.Value
                    elseif slot == 3 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot3.Value
                    elseif slot == 4 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot4.Value
                    elseif slot == 5 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot5.Value
                    elseif slot == 6 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot6.Value
                    end
                end
            end
        end)
    end
    
    -- check if there's money unit
    
    function checkmoneyunit()
        pcall(function()
            if unit == "Bellma" or unit == "Pride (Ultra)" then
                repeat wait()
                    random = math.random(1,6)
                    slot = random
                    if slot == 1 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot1.Value
                    elseif slot == 2 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot2.Value
                    elseif slot == 3 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot3.Value
                    elseif slot == 4 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot4.Value
                    elseif slot == 5 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot5.Value
                    elseif slot == 6 then
                        unit = game:GetService("ReplicatedStorage").Player.Slot6.Value
                    end
                until unit ~= "Bellma" and unit ~= "Pride (Weak)" and unit ~= "Pride (Ultra)"
            end
        end)
    end
    
    -- place unit
    
    function place()
        pcall(function()
            for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do wait()
                if v.ClassName == "Model" then
                    game:GetService("ReplicatedStorage").Remotes.Place:FireServer(v.HumanoidRootPart.CFrame * CFrame.new(0,0,-40),unit,nil) 
                    game.StarterGui:SetCore("SendNotification", {
                        Title = "EZ NOOB HUB"; -- the title (ofc)
                        Text = "Placed " ..unit; -- what the text says (ofc)
                        Icon = ""; -- the image if u want. 
                        Duration = 5; -- how long the notification should in secounds
                    })
                end
            end
        end)
    end
    
    -- play
    
    function play()
        while getgenv().autoplace == true do wait()
            pcall(function() 
                repeat wait()
                getunit()
                checkmoneyunit()
                    for i,v in pairs(game:GetService("ReplicatedStorage").Units:GetDescendants()) do
                        if v.Name == unit then
                            for i2,v2 in pairs (v:GetChildren()) do
                                if v2.Name == "Cost" then
                                    for i3,v3 in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do wait()
                                        if v3.ClassName == "Model" then
                                        place()
                                        end
                                    end
                                end
                            end
                        end
                    end
                until game:GetService("Players").Vixaus.PlayerGui.MainUI.Tokens >= v2.Value and getgenv().autoplace == false
            end)
        end
    end
    
    play()

else
    game.Players.LocalPlayer:Kick("This Game is not support")
end
