local Tab = Window:NewTab("UI")

local Section = Tab:NewSection("UI Setting")

Section:NewKeybind("Press LeftControl To Toggle UI", "Press the key to turn on/off ui", Enum.KeyCode.LeftControl, function()
	Library:ToggleUI()
end)
